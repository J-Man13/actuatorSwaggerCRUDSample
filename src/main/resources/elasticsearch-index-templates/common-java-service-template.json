//This file is not used anywhere
//Just wanted to show java-service-logs index mapping
//Used lower mentioned json as body for index template update
{
  "index_patterns": [
    "java-service-*"
  ],
  "settings": {
    "index": {
      "refresh_interval": "10s"
    }
  },
  "mappings": {
    "properties": {
      "activityId": {
        "type": "keyword"
      },
      "correlationId": {
        "type": "keyword"
      },
      "appArchiveName": {
        "type": "keyword"
      },
      "logDateStamp": {
        "type": "date",
        "format": "yyyy-MM-dd HH:mm:ss.SSS"
      },
      "thread": {
        "type": "keyword"
      },
      "level": {
        "type": "keyword"
      },
      "loggerName": {
        "type": "keyword"
      },
      "endOfBatch": {
        "type" : "boolean"
      },
      "loggerFqcn": {
        "type" : "keyword"
      },
      "threadId": {
        "type": "long"
      },
      "threadPriority" : {
        "type" : "long"
      },
      "message": {
        "properties":{
          "logCallLocation" : {
            "type" : "text"
          },
          "logCauseDescription":{
            "type" : "text"
          },
          "logEntry" : {
            "type" : "object"
          },
          "logMap" : {
            "type" : "object"
          }
        },
        "type" : "object"
      }
    }
  }
}