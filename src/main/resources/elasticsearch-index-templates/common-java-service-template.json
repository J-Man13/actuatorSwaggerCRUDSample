{
  "index_patterns": [
    "java-service-*"
  ],
  "settings": {
    "index": {
      "refresh_interval": "10s"
    }
  },
  "mappings": {
    "properties": {
      "activityId": {
        "type": "keyword"
      },
      "correlationId": {
        "type": "keyword"
      },
      "appArchiveName": {
        "type": "keyword"
      },
      "logDateStamp": {
        "type": "date",
        "format": "yyyy-MM-dd HH:mm:ss.SSS"
      },
      "level": {
        "type": "keyword"
      },
      "logCallLocation": {
        "type": "text"
      },
      "logHostName": {
        "type": "keyword"
      },
      "logHostAddress": {
        "type": "keyword"
      },
      "logCauseDescription": {
        "type": "text"
      },
      "logEntry": {
        "type": "object"
      },
      "logMap": {
        "type": "object"
      }
    },
    "dynamic_templates": [{
      "strings_as_text": {
        "match_mapping_type": "string",
        "mapping": {
          "type": "text"
        }
      }
    }
    ]
  }
}